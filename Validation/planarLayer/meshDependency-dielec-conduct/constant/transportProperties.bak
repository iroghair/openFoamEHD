FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 6-8//

phase1
{
    transportModel  Newtonian;
    nu              nu [0 2 -1 0 0 0 0] 6.1e-05;
    rho             rho [1 -3 0 0 0 0 0] 900;
    sgm             sgm [-1 -3 3 0 0 2 0] 8.05e-9;
    eps             eps [-1 -3 4 0 0 2 0] 2.7e-11;
}

phase2
{
    transportModel  Newtonian;
    nu              nu [0 2 -1 0 0 0 0] 1.48e-05;
    rho             rho [1 -3 0 0 0 0 0] 1;
    sgm             sgm [-1 -3 3 0 0 2 0] 1.05e-12;
    eps             eps [-1 -3 4 0 0 2 0] 8.85419e-12;
}

sigma           sigma [1 0 -2 0 0 0 0] 0.07;

