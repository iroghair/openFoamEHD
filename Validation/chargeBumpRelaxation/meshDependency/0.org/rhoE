FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      rhoE;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 1 0 0 1 0];

internalField   uniform 0; 

boundaryField
{
    upWall
    {
        type            zeroGradient;
    }
    downWall
    {
        type            zeroGradient;
    }
    leftandright
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}


